 <div class= "totalAlbums card">
    <div class="row">
        {{#each totalAlbumsArr}}
             <div class="name">{{this.name}}</div>
             <img class="img" src="{{this.images.[0].url}}" alt="{{this.name}}">
             <a href="/tracks/{{this.id}}">Tracks</a>         
        {{/each}}
    </div>
</div>